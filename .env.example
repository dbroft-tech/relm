# Django Settings
# Generate a new secret key and paste it here
DJANGO_SECRET_KEY=generate-a-secure-key-here
DJANGO_DEBUG=False
DJANGO_ALLOWED_HOSTS=.vercel.app,.now.sh,localhost,127.0.0.1

# Email Settings
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USE_TLS=True
EMAIL_HOST_USER=your-email@example.com
EMAIL_HOST_PASSWORD=your-app-specific-password

# Site Settings
SITE_NAME=Redeemed Life Church
SITE_URL=https://your-domain.vercel.app
SITE_DESCRIPTION=Welcome to Redeemed Life Church

# Vercel Analytics
VERCEL_ANALYTICS_ID=your-analytics-id
VERCEL_SPEED_INSIGHTS_ID=your-speed-insights-id 